<?xml version="1.0" encoding="UTF-8"?>
<map
	map_file="maps/world.swf"
	zoom_x="0%"
	zoom_y="0%"
	zoom="100%"
>
  <areas>
		<% @countries.each do |country| -%>
			<area title="<%=h country.name %>" mc_name="<%=h country.name_ammap %>" value="1"></area>
		<% end %>
      <area title="borders" mc_name="borders" color="#FFFFFF" balloon="false"></area>
  </areas>
</map>
